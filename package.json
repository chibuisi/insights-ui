{
  "name": "minor-insights-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode intg",
    "build:intg": "vue-cli-service build --mode intg",
    "build:stge": "vue-cli-service build --mode stge",
    "build:prod": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "dev:local": "vue-cli-service serve --open --host localhost --port 20340 --config-env local",
    "dev:intg": "vue-cli-service serve --mode development --open --host localhost --port 20340 --config-env intg",
    "dev:stge": "vue-cli-service serve --mode production --open --host localhost --port 20340 --config-env stge",
    "dev:prod": "vue-cli-service serve --mode production --open --host localhost --port 20340 --config-env prod"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "axios": "^1.3.6",
    "core-js": "^3.6.5",
    "global": "^4.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jwt-decode": "^3.1.2",
    "serve": "^14.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^4.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "svg-sprite-loader": "^6.0.11",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6"
  }
}
